Executions_Webhook = "https://discord.com/api/webhooks/1340796672254541964/CHOXlXe5GWUm3QEYz0Kd07Dmc2okMRqlyCRoHU5A9ERCg5Ot-GR-fhnrrqBKOjjaRDhQ"
Username = "nbvbw"
